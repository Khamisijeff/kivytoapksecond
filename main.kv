<Mainscreenunits>:
    size_hint:None,None
    width:dp(190)
    height:self.width/1.3
    radius:dp(15)
    id:report_pad
    padding:dp(20)
    elevation:3
    pos_hint:{'center_x':0.5,'center_y':0.5}
    #md_bg_color:root.pad_color
    md_bg_color:'white'

    MDFloatLayout:
        padding:dp(10)
        pos_hint:{'center_x':0.5,'center_y':0.87}
        size_hint:1,0.2
        MDIcon:
            icon:'account'
            theme_text_color:'Custom'
            text_color:'black'
            pos_hint:{'center_x':.9,'center_y':0.5}
        MDTextButton:
            text:root.nametxt
            halign:'center'
            id:Neighbor_Name
            font_name:'bijan.ttf'
            theme_text_color:'Custom'
            text_color: 'black'
            font_size:'15sp'
            pos_hint:{'center_x':.5,'center_y':0.5}
            bold:True


    MDFloatLayout:
        padding:dp(10)
        pos_hint:{'center_x':0.5,'center_y':0.68}
        size_hint:1,0.2

        MDIcon:
            icon:'calendar'
            theme_text_color:'Custom'
            text_color:'black'
            pos_hint:{'center_x':0.9,'center_y':0.5}
        MDTextButton:
            text:root.enterance_date
            halign:'right'
            pos_hint:{'center_x':0.5,'center_y':0.5}
            theme_text_color:'Custom'
            text_color:'black'
            font_size:'15sp'
            id:enterance_Date
            font_name:'bijan.ttf'
            bold:True
            on_press:print(self.text)
    MDFloatLayout:
        size_hint:None,None
        md_bg_color:'white'
        width:self.parent.width/1.1
        height:self.width/3
        radius:dp(10)
        padding:dp(20)
        pos_hint:{'center_x':0.5,'center_y':0.3}
        MDFloatLayout:
            size_hint:None,None
            width:self.parent.width/5
            height:self.width
            radius:dp(30)
            pos_hint:{'center_x':0.87,'center_y':0.5}
            md_bg_color:colors[app.theme_cls.primary_palette]['500']
            MDTextButton:
                text:root.unit_number
                id:unit_number
                theme_text_color:'Custom'
                font_size:self.parent.width/1.5
                text_color:'white'
                font_name:'bijan.ttf'
                pos_hint:{'center_x':0.5,'center_y':0.55}

        MyIconButton:
            icon:'cash-plus'
            theme_text_color:'Custom'
            md_bg_color:colors[app.theme_cls.primary_palette]['500']
            icon_size:'14sp'
            text_color:'white'
            pos_hint:{'center_x':0.63,'center_y':0.5}
            on_press:root.get_PaymentList()
        MDLabel:
            text:root.phone
            opacity:0
            id:unitcellphone

        MyIconButton:
            icon:'phone'
            theme_text_color:'Custom'
            text_color:'white'
            md_bg_color:colors[app.theme_cls.primary_palette]['500']
            icon_size:'13sp'
            pos_hint:{'center_x':0.38,'center_y':0.5}
            on_press:root.getunitphonenumber(root.ids.unitcellphone.text)
        MyIconButton:
            icon:'menu'
            md_bg_color:colors[app.theme_cls.primary_palette]['500']
            icon_size:'13sp'
            theme_text_color:'Custom'
            font_size:self.parent.width/1.5
            text_color:'white'
            pos_hint:{'center_x':0.15,'center_y':0.5}
            on_press:root.getunitreportpayment(root.ids.unit_number.text)
